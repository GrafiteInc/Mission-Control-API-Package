{
    "name": "grafite/mission-control-api",
    "description": "The PHP agnostic API package for Mission Control's PRO API (Grafite Inc)",
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Lantz",
            "email": "matt@grafite.ca"
        }
    ],
    "require": {
        "php": ">=7.1",
        "mashape/unirest-php": "^3.0",
        "nesbot/carbon": "^1.25",
        "tightenco/collect": "^5.6"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Grafite\\MissionControlApi\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "stable"
}
